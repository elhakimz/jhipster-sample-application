<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.9.xsd">
    <!--
        Added the constraints for entity PartyFacility.
    -->
    <changeSet id="20201023033235-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="party_id"
                                 baseTableName="party_facility"
                                 constraintName="fk_party_facility_party_id"
